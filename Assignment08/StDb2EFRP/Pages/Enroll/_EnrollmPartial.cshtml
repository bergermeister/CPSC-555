@*page*@
@model List<StDb2EFRP.Models.ViewModels.EnrollmentVM>
<p>
   <a asp-page="./Register" asp-route-cnum="@Model[0].CourseNum">Register for Course</a>
</p>
<table class="table">
   <thead>
      <tr><th>First Name</th><th>Last Name</th><th>Student Id</th><th>Credits</th><th></th></tr>
   </thead>
   <tbody>
      @foreach( var item in Model )
      {
         <tr>
            <td>
               @Html.DisplayFor( modelItem => item.FirstName )
            </td>
            <td>
               @Html.DisplayFor( modelItem => item.LastName )
            </td>
            <td>
               @Html.DisplayFor( modelItem => item.StudentId )
            </td>
            <td>
               @Html.DisplayFor( modelItem => item.Credits )
            </td>
            <td>
            <td>
               <a asp-page="../Stu/Details" asp-route-id="@item.StudentId">Details</a> |
               <a asp-page="./Delete" asp-route-id="@item.StudentId">Delete</a>
            </td>
         </tr>
      }
   </tbody>
</table>